{
    "name": "r7booking/ishu8957",
    "description": "booking library for laravel and lumen",
    "keywords": ["framework", "laravel", "lumen","bookings"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ishu Sharma",
            "email": "ishu8957@outlook.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "irazasyed/larasupport": "^1.7",
        "laravel/lumen-framework": "^8.0",
        "rinvex/laravel-support": "^5.0",
        "stripe/stripe-php": "7.72.0"
    },
    "require-dev": {
        "flipbox/lumen-generator": "^8.2",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "R7\\Booking\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R7\\Booking\\Providers\\BookingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
